    <tu>
      <tuv xml:lang="EN">
        <seg>Unlikely</seg>
      </tuv>
      <tuv xml:lang="ES" changeid="hcartagena" changedate="20141014T143103Z" creationid="hcartagena" creationdate="20141014T143103Z">
        <seg>Poco probable</seg>
      </tuv>
